<odoo>
    <data>
        <!-- Form view -->
        <record id="ownership_form_view" model="ir.ui.view">
            <field name="name">ownership.form</field>
            <field name="model">ownership.commerce</field>
            <field name="arch" type="xml">
                <form string="Ownership Form">
                    <sheet>
                        <group>
                            <field name="start_date"/>
                            <field name="end_date"/>
                            <field name="is_current"/>
                            <field name="name_owner_commerce_ids"/>
                            <field name="ownership_mall" />
                             <field name="store_unit_ids"/>
                            <field name="ownership_files"/>
                           
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

    </data>
</odoo>
